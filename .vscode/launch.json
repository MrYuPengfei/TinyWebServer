    "version": "0.2.0",
    "configurations": [
        {
            // "sudo":true,
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/server",
            // -p 9007 -l 1 -m 0 -o 1 -s 10 -t 10 -c 1 -a 1
            // "args": ["-p", "9007", "-l", "2", "-m", "0", "-o", "1", "-s", "10", "-t", "10", "-c", "1", "-a", "1"],
            "args":[],
            "stopAtEntry": true, 
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }


    ]
}